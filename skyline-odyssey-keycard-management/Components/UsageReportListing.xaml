 <UserControl x:Class="skyline_odyssey_keycard_management.Components.UsageReportListing"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:skyline_odyssey_keycard_management.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DataGrid x:Name="reportDataGrid" Grid.Row="0" Margin="0 20" ItemsSource="{Binding UsageReportsListingItemViewModels}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Name" Binding="{Binding Name}" />
                <DataGridTextColumn Header="AccessLevel" Binding="{Binding AccessLevel}" />
                <DataGridTextColumn Header="Timestamp" Binding="{Binding Timestamp}" />
                <DataGridTextColumn Header="Keycard" Binding="{Binding Keycard}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>

</UserControl>
